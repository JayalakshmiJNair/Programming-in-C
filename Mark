#include<stdio.h>
void main()
{
   int n,i;
   printf("Enter the number of students\n");
   scanf("%d",&n);
   int marks[n];
   printf("Enter the marks for %d students\n",n);
   for(i=0;i<n;i++)
   {
     printf("Enter the marks for students %d\n",i+1);
     scanf("%d",&marks[i]);
   }
   printf("Marks of %d students\n",n);
   for(i=0;i<n;i++)
   {
     printf("Student %d:%d\n",i+1,marks[i]);
   }
}
   
