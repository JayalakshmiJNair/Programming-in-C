#include<stdio.h>
void main()
{
   int a[2][3];
   printf("Enter 2x3 matrix\n");
   for(int i=0;i<2;i++)
   {
        for(int j=0;j<3;j++)
        
        {
           scanf("%d",&a[i][j]);
        }
    }
    // to print the matrix
   for(int i=0;i<2;i++)
   
     {
        for(int j=0;j<3;j++)
        
     {
        printf("%d\t",a[i][j]);
     }
        printf("\n");
     }
 }   
   
   
