#include<stdio.h>
void main(){
     int no,rem,rev=0,dup;
     printf("Enter a number");
     scanf("%d",&no);
     dup=no;
     while(no>0){
         rem=no%10;
         rev=rev*10+rem;
         no=no/10;
       }
       if(rev==dup)
          printf("Palindrome");
          else
            printf("Not palindrome");
         
    }
          
