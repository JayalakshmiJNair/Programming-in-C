#include <stdio.h>

int main() {
    int limit;
    
    // Prompt user to enter the limit
    printf("Enter the limit: ");
    scanf("%d", &limit);
    
    // Print odd numbers up to the limit
    printf("Odd numbers up to %d are:\n", limit);
    for (int i = 1; i <= limit; i += 2) {
        printf("%d ", i);
    }
    
    printf("\n");
    return 0;
}
