#include<stdio.h>
void main(){
   int r,c,a[100][100];
   printf("Enter the number of rows and columns ");
   scanf("%d%d",&r,&c);
   printf("Enter the elements ");
   for(int i=0;i<r;i++){
       for(int j=0;j<c;j++){
          scanf("%d",&a[i][j]);
        }
     }     
     printf("Matrix\n");
     for(int i=0;i<r;i++){
       for(int j=0;j<c;j++){
          printf("%d\t",a[i][j]);
        }
        printf("\n");
     }
     printf("Transpose\n");
     for(int i=0;i<c;i++){
       for(int j=0;j<r;j++){
          printf("%d\t",a[j][i]);
        }
        printf("\n");
     }
 }     
